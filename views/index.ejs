  <section class="contact-us">
    <div class="container">
      <div class="contact-box">
        <div class="row">
          <div class="col-lg-6">
            <div class="contact-img-box">
              <img src="../images/Contact-banner.png" alt="Contact Banner" class="img-fluid">
            </div>
          </div>
          <div class="col-lg-6 btn-group-vertical">
            <div class="contact-box-content">
              <h1>Need Help?</h1>
              <a href="#" class="btn-contact">Contact A Medical Professional</a>
